{
  "version": 2,
  "builds": [
    {
      "src": "frontend/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/app.js",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/RADPAIR-LOGO-WHITE.png",
      "dest": "/public/RADPAIR-LOGO-WHITE.png"
    },
    {
      "src": "/app.js",
      "dest": "/frontend/app.js"
    },
    {
      "src": "/styles.css",
      "dest": "/frontend/styles.css"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html"
    }
  ],
  "env": {
    "NEXT_PUBLIC_WS_URL": "@radpair_ws_url"
  }
}